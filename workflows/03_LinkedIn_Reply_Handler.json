    {
      "parameters": {
        "jsCode": "const original = $('Telegram Trigger').item.json.message.reply_to_message.text;\n// Regex para sacar texto entre las l\u00edneas punteada\nconst regex = /-{10,}\\n([\\s\\S]*?)\\n-{10,}/;\nconst match = original.match(regex);\n\nlet content = \"\";\nif (match && match[1]) {\n  content = match[1].trim();\n} else {\n  // Fallback por si cambia el formato\n  content = original;\n}\n\nreturn { json: { content } };"
      },
